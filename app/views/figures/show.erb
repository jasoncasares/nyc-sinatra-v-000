<% unless locals.empty? %>
  <%= message %>
<% end %>
<h1><strong style="color: red">Figure</strong>: <%= @figure.name %></h1>
<h3><strong style="color: blue">Titles</strong>:</h3>
<% @figure.titles.each do |title| %>
  <p><%=title.name%></p>
<% end %>
<h3><strong style="color: green">Landmarks</strong>:</h3>
<% @figure.landmarks.each do |landmark| %>
  <p><%=landmark.name%></p>
<% end %>
<a href="/figures/<%=@figure.id%>/edit">Edit Me</a>
